# Series test

require_relative 'SeriesMasterOfTheUniverse'

smou = SeriesMasterOfTheUniverse.new

puts "Worst: " + smou.worst_series( [ "Star Trek", "Firefly", "Big Bang", "Lost" ] ).to_s
puts "Best: " + smou.best_series( [ "Star Trek", "Firefly", "Big Bang", "Lost" ] ).to_s

smou.best_of_series.keys.each do |key|

	puts "Series: " + key.to_s + ", Rating: " + smou.last_search_array[key].to_s

end